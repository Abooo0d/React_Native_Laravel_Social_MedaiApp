{
  "expo": {
    "name": "Chatter",
    "slug": "Chatter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo.png",
    "backgroundColor": "#111827",
    "scheme": "reactnativesocialmediaapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Logo.png",
        "backgroundColor": "#111827"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.ReactNativeSocialMediaApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Logo.png",
          "imageWidth": 150,
          "resizeMode": "cover",
          "backgroundColor": "#111827"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
